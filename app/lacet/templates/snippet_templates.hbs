<script type="text/template"  klass='Snippet' kind='bootstrap.panel' name="Text Panel" icon="fa fa-edit" category="Media">
    <!-- Text Panel -->
    <div class="panel {{panelColor}}">
        <div class="panel-heading editable-header">{{{header}}}</div>
        <div class="panel-body editable-text">{{{text}}}</div>
        <div class="panel-footer editable-footer">{{{footer}}}</div>
    </div>
</script>

<script type="text/template" kind='bootstrap.carousel' klass='Snippet' name="Carousel" icon="fa fa-edit" category="Media">
    <!-- Carousel -->
    <div id="{{id}}" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            {{#each carouselList}}
                <li data-target="#{{@root.id}}" data-slide-to="{{@index}}" class="test {{#if @first}}active{{/if}}" ></li>
            {{/each}}
        </ol>
        <div class="carousel-inner" role="listbox" >
            {{#each carouselList}}
                <div class="item {{#if @first}}active{{/if}}">
                    {{#if  @root.scale }} <div class="container">  {{/if}}
                        <div class="carousel-caption">
                            <h4>{{{header}}}</h4>
                            <p> {{{text}}}</p>
                        </div>
                {{#if @root.scale}} </div>  {{/if}}
                {{#if @root.scale}} <div class="header-back">  {{/if}}
                        <img alt="Carousel Bootstrap First" src="{{image}}"/>
                {{#if @root.scale}}  </div>   {{/if}}
                </div>
            {{/each}}
        </div>
        <a class="left carousel-control" data-target="#{{id}}" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"  aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a  class="right carousel-control" data-target="#{{id}}" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"  aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</script>
